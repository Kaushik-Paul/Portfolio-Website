{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/index.html",
    "/ai-agents.html",
    "/stock-picker.html",
    "/ai-coder.html",
    "/career-conversation.html",
    "/style.css",
    "/app.js",
    "/resources/*"
  ],
  "routes": [
    {"src": "/ai-agents", "dest": "/ai-agents.html", "status": 200},
    {"src": "/stock-picker", "dest": "/stock-picker.html", "status": 200},
    {"src": "/ai-coder", "dest": "/ai-coder.html", "status": 200},
    {"src": "/career-conversation", "dest": "/career-conversation.html", "status": 200},
    {"src": "/*", "dest": "/index.html", "status": 200}
  ]
}
