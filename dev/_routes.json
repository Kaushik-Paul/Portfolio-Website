{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/index.html",
    "/ai-agents.html",
    "/stock-picker.html",
    "/ai-coder.html",
    "/career-conversation.html",
    "/style.css",
    "/app.js",
    "/resources/*"
  ],
  "routes": [
    {"handle": "rewrite", "src": "/ai-agents/?*", "dest": "/ai-agents.html"},
    {"handle": "rewrite", "src": "/stock-picker/?*", "dest": "/stock-picker.html"},
    {"handle": "rewrite", "src": "/ai-coder/?*", "dest": "/ai-coder.html"},
    {"handle": "rewrite", "src": "/career-conversation/?*", "dest": "/career-conversation.html"},
    {"src": "/*", "dest": "/index.html", "status": 200}
  ]
}
