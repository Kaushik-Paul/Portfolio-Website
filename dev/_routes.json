{
  "version": 1,
  "include": ["/*"],
  "exclude": [
    "/*",
    "/index.html",
    "/redirect.html",
    "/style.css",
    "/app.js",
    "/resources/*"
  ],
  "routes": [
    {"handle": "rewrite", "src": "/ai-agents(/?)*", "dest": "/redirect.html?project=ai-agents"},
    {"handle": "rewrite", "src": "/stock-picker(/?)*", "dest": "/redirect.html?project=stock-picker"},
    {"handle": "rewrite", "src": "/ai-coder(/?)*", "dest": "/redirect.html?project=ai-coder"},
    {"handle": "rewrite", "src": "/career-conversation(/?)*", "dest": "/redirect.html?project=career-conversation"},
    {"handle": "filesystem"}
  ]
}
